# code: language=yaml
#
# Extend $observed with IPAM-related state
# Simplified: IPAM pools are no longer created, subnets use cidrmath
# Depends on: 000-observed-vpc.yaml.gotmpl ($observed)
#

# Build minimal IPAM observed state for compatibility
# Subnet CIDRs are now calculated via cidrmath, not observed from IPAM allocations
{{ $observed = merge $observed (dict "ipam" (dict
  "ipv4" (dict
    "enabled" false
  )
  "ipv6Ula" (dict
    "enabled" false
  )
)) }}
